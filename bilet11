#include <iostream>
#include <string>
#include <cstdlib>
#include <ctime>

using namespace std;

int main ()
{
	int n,i;
	int arr[n];
	srand(time(NULL));
	cout<<"vvedite n";
	cin>>n;
	for(int i=0;i<n;i++)
	{
		arr[i]=rand() % 10-1;
		cout<< "arr["<<arr[i]<<"]"<<endl;
			}
			if(arr[0]<arr[1]) cout<<"povishenie";
			else cout<<"ponishenie";
			cout<<"\n";
			system ("pause");
			return 0;
}
